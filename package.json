{
  "name": "nuxtedomoon",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "Nuxtedo-Moon is a portfolio/blog Nuxt template designed using Nuxt-Content, Tailwind CSS and daisyUI",
  "author": "khatastroffik",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/khatastroffik/nuxtedomoon.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/khatastroffik/treefolder/issues"
  },
  "keywords": [
    "typescript",
    "Nuxt",
    "portfolio",
    "khatastroffik",
    "blog",
    "template",
    "TailwindCSS",
    "daisyUI",
    "k11k"
  ],
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "prep": "nuxt prepare",
    "prepare": "husky",
    "lint": "eslint .",
    "lintix": "eslint . --fix",
    "dry-commit": "git hook run pre-commit",
    "cleanup": "nuxt cleanup",
    "reset": "nuxt cleanup && pnpx rimraf .nuxt .output .data dist node_modules && pnpm install"
  },
  "dependencies": {
    "@nuxt/content": "^3.11.2",
    "@nuxtjs/tailwindcss": "6.14.0",
    "better-sqlite3": "^12.6.2",
    "nuxt": "^4.3.1",
    "vue": "^3.5.28",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.2.4"
  },
  "lint-staged": {
    "*": "pnpm lint"
  }
}
